{
  "name": "meilv-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:cloudflare": "next build && npx @opennextjs/cloudflare@latest build",
    "start": "next start",
    "lint": "eslint",
    "init-db": "node scripts/init-supabase-data.js",
    "preview": "npm run build:cloudflare && wrangler pages dev .open-next/assets --compatibility-date=2025-03-25",
    "deploy": "npm run build:cloudflare && wrangler pages deploy .open-next/assets",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.56.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@opennextjs/cloudflare": "^1.3.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.24.4"
  }
}
